      "\t\t<el-menu-item v-for=\"(${7:item}, ${8:subIndex}) in ${4:submenus}\"",
      "\t\t\t:index=\"(${5:index} + 1) + '-' + (${8:subIndex} + 1)\">",
      "\t\t\t\t{{${7:item}.${9:title}}}",
      "\t\t</el-menu-item>",
