{
  "Default Template": {
    "prefix": "vtemp",
    "body": [
      "<template>",
      "\t<div></div>",
      "</template>",
      "<script>",
      "export default {",
      "\tname: '$1',",
      "\tdata() {",
      "\t\treturn {}",
      "\t}",
      "}",
      "</script>",
      "<style lang=\"$2\"></style>",
      "${3}"
    ],
    "description": "Vue default template"
  },
